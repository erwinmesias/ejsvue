  "bugs": {
    "url": "https://github.com/erwinmesias/ejsvue/issues"
  },
  "homepage": "https://github.com/erwinmesias/ejsvue#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.5.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "vue": "^2.6.11",
    "vue-file-agent": "^1.7.3"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "laravel-mix": "^2.1.14",
    "node-sass": "^4.10.0"
  }
}
